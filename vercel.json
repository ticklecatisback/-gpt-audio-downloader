{
  "builds": [
    {
      "src": "image_downloader.py",
      "use": "@vercel/python"
    },
    {
      "src": "add.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/image-downloader/(.*)",
      "dest": "image_downloader.py"
    },
    {
      "src": "/add/(.*)",
      "dest": "add.py"
    }
  ]
}
